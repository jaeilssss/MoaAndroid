<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

<data>
    <variable
        name="model"
        type="com.moa.moakotlin.ui.concierge.category.NeederMainCategoryViewModel" />
</data>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<androidx.constraintlayout.widget.ConstraintLayout

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/NeederMainCategoryEduLayout"
        android:background="@drawable/concierge_main_selector_view"
        app:layout_constraintDimensionRatio="H,1:1"
        android:layout_margin="15dp"
        android:id="@+id/NeederMainCategoryKidLayout"
        app:layout_constraintTop_toTopOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintStart_toStartOf="@id/NeederMainCategoryKidLayout"
            app:layout_constraintEnd_toEndOf="@id/NeederMainCategoryKidLayout"
            app:layout_constraintTop_toTopOf="@id/NeederMainCategoryKidLayout"
            app:layout_constraintBottom_toBottomOf="@id/NeederMainCategoryKidLayout"
            app:layout_constraintDimensionRatio="H,1:1">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintDimensionRatio="H,1:1"
                android:layout_margin="15dp"
                android:src="@drawable/ic_category_kid"
                android:id="@+id/NeederMainCategoryKid"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="육아"
                android:fontFamily="@font/notosanscjkkr_medium"
                app:layout_constraintStart_toStartOf="@id/NeederMainCategoryKid"
                app:layout_constraintEnd_toEndOf="@id/NeederMainCategoryKid"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textColor="#000000"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/NeederMainCategoryEduLayout"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintStart_toEndOf="@id/NeederMainCategoryKidLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_margin="15dp"
        android:background="@drawable/concierge_main_selector_view">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintDimensionRatio="H,1:1"
                android:layout_margin="15dp"
                android:src="@drawable/ic_category_edu"
                android:id="@+id/NeederMainCategoryEdu"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="교육"
                android:fontFamily="@font/notosanscjkkr_medium"
                app:layout_constraintStart_toStartOf="@id/NeederMainCategoryEdu"
                app:layout_constraintEnd_toEndOf="@id/NeederMainCategoryEdu"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textColor="#000000"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/NeederMainCategoryKidLayout"
        app:layout_constraintEnd_toEndOf="@id/NeederMainCategoryKidLayout"
        app:layout_constraintTop_toBottomOf="@id/NeederMainCategoryKidLayout"
        android:id="@+id/NeederMainCategoryPetLayout"
        app:layout_constraintDimensionRatio="H,1:1"
        android:background="@drawable/concierge_main_selector_view"
        android:layout_marginTop="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintDimensionRatio="H,1:1"
                android:layout_margin="15dp"
                android:src="@drawable/ic_category_pet"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="반려동물케어"
                android:fontFamily="@font/notosanscjkkr_medium"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/NeederMainCategoryEduLayout"
        app:layout_constraintEnd_toEndOf="@id/NeederMainCategoryEduLayout"
        app:layout_constraintTop_toBottomOf="@id/NeederMainCategoryEduLayout"
        android:layout_marginTop="15dp"
        app:layout_constraintDimensionRatio="H,1:1"
        android:background="@drawable/concierge_main_selector_view"
        android:id="@+id/NeederMainCategoryInteriorLayout">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_margin="20dp"
                app:layout_constraintDimensionRatio="H,1:1"

                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:src="@drawable/ic_category_interior"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="인테리어"
                android:fontFamily="@font/notosanscjkkr_medium"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/NeederMainCategoryPetLayout"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintEnd_toEndOf="@id/NeederMainCategoryPetLayout"
        app:layout_constraintTop_toBottomOf="@id/NeederMainCategoryPetLayout"
        android:layout_marginTop="15dp"
        android:background="@drawable/concierge_main_selector_view"
        android:id="@+id/NeederMainCategorySharingLayout">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="H,1:1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:src="@drawable/ic_category_etc"
                android:layout_margin="15dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="품앗이"
                android:fontFamily="@font/notosanscjkkr_medium"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintDimensionRatio="H,1:1"
    app:layout_constraintStart_toStartOf="@id/NeederMainCategoryInteriorLayout"
    app:layout_constraintEnd_toEndOf="@id/NeederMainCategoryInteriorLayout"
    android:background="@drawable/concierge_main_selector_view"
    app:layout_constraintTop_toBottomOf="@id/NeederMainCategoryInteriorLayout"
    android:id="@+id/NeederMainCategoryBorrowLayout"
    android:layout_marginTop="15dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_margin="3dp"
        android:background="@drawable/shape_grey_radius_10">
        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="H,1:1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/ic_category_etc"
            android:layout_margin="15dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:text="빌려주세요"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:textColor="@color/moaBlack"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintStart_toStartOf="@id/NeederMainCategorySharingLayout"
        app:layout_constraintEnd_toEndOf="@id/NeederMainCategorySharingLayout"
        android:background="@drawable/concierge_main_selector_view"
        app:layout_constraintTop_toBottomOf="@id/NeederMainCategorySharingLayout"
        android:id="@+id/NeederMainCategoryEtcLayout"
        android:layout_marginTop="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_margin="3dp"
            android:background="@drawable/shape_grey_radius_10">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="H,1:1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:src="@drawable/ic_category_etc"
                android:layout_margin="15dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="기타"
                android:fontFamily="@font/notosanscjkkr_medium"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</layout>