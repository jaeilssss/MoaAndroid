<?xml version="1.0" encoding="utf-8"?>
<layout>

<data>

</data>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<androidx.constraintlayout.widget.ConstraintLayout

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.claim.ClaimCategorySelectActivity">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/back"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="15dp"
            android:padding="5dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:layout_width="12dp"
                android:layout_height="25dp"
                android:background="@drawable/ic_back"
                android:scaleType="fitXY"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/back"
            app:layout_constraintBottom_toBottomOf="@id/back"
            android:text="카테고리 선택"
            android:textColor="@color/moaBlack"
            android:textSize="18dp"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:textStyle="bold"
            android:id="@+id/ClaimCategoryText"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/success"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="@id/back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/back"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:text="완료"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/moaBlack"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:includeFontPadding="false"
            android:layout_marginEnd="15dp"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/ClaimMainCategoryParkLayout"
        android:background="@drawable/concierge_main_selector_view"
        app:layout_constraintDimensionRatio="H,1:1"
        android:layout_margin="15dp"
        android:id="@+id/ClaimMainCategoryelevatorLayout"
        app:layout_constraintTop_toBottomOf="@id/ClaimCategoryText">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="3dp"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintStart_toStartOf="@id/ClaimMainCategoryelevatorLayout"
            app:layout_constraintEnd_toEndOf="@id/ClaimMainCategoryelevatorLayout"
            app:layout_constraintTop_toTopOf="@id/ClaimMainCategoryelevatorLayout"
            app:layout_constraintBottom_toBottomOf="@id/ClaimMainCategoryelevatorLayout"
            app:layout_constraintDimensionRatio="H,1:1">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:padding="20dp"
                app:layout_constraintDimensionRatio="H,1:1"
                app:layout_constraintBottom_toTopOf="@id/ClaimMainCategoryelevatorText"
                android:src="@drawable/img_elevator_active"
                android:id="@+id/ClaimMainCategoryelevator"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="승강기"
                android:textSize="15dp"
                android:fontFamily="@font/notosanscjkkr_medium"
                app:layout_constraintStart_toStartOf="@id/ClaimMainCategoryelevator"
                app:layout_constraintEnd_toEndOf="@id/ClaimMainCategoryelevator"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryelevator"
                android:id="@+id/ClaimMainCategoryelevatorText"
                android:textColor="#000000"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/ClaimMainCategoryParkLayout"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintStart_toEndOf="@id/ClaimMainCategoryelevatorLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ClaimCategoryText"
        android:layout_margin="15dp"
        android:background="@drawable/concierge_main_selector_view">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintDimensionRatio="H,1:1"
                android:padding="20dp"
                android:src="@drawable/img_park_active"
                app:layout_constraintBottom_toTopOf="@id/ClaimMainCategoryParkText"
                android:id="@+id/ClaimMainCategoryPark"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="주차장"
                android:textSize="15dp"
                android:id="@+id/ClaimMainCategoryParkText"
                app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryPark"
                android:fontFamily="@font/notosanscjkkr_medium"
                app:layout_constraintStart_toStartOf="@id/ClaimMainCategoryPark"
                app:layout_constraintEnd_toEndOf="@id/ClaimMainCategoryPark"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textColor="#000000"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/ClaimMainCategoryelevatorLayout"
        app:layout_constraintEnd_toEndOf="@id/ClaimMainCategoryelevatorLayout"
        app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryelevatorLayout"
        android:id="@+id/ClaimMainCategorySecurityLayout"
        app:layout_constraintDimensionRatio="H,1:1"
        android:background="@drawable/concierge_main_selector_view"
        android:layout_marginTop="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">

            <ImageView
                android:id="@+id/ClaimMainCategorySecurity"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/img_security_active"
                android:padding="20dp"
                app:layout_constraintBottom_toTopOf="@id/ClaimMainCategorySecurityText"
                app:layout_constraintDimensionRatio="H,1:1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.511"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="보안/경비"
                android:textSize="15dp"
                android:id="@+id/ClaimMainCategorySecurityText"
                app:layout_constraintTop_toBottomOf="@id/ClaimMainCategorySecurity"
                android:fontFamily="@font/notosanscjkkr_medium"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/ClaimMainCategoryParkLayout"
        app:layout_constraintEnd_toEndOf="@id/ClaimMainCategoryParkLayout"
        app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryParkLayout"
        android:layout_marginTop="15dp"
        app:layout_constraintDimensionRatio="H,1:1"
        android:background="@drawable/concierge_main_selector_view"
        android:id="@+id/ClaimMainCategoryLandscapeLayout">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="H,1:1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/ClaimMainCategoryLandscape"
                app:layout_constraintBottom_toTopOf="@id/ClaimMainCategoryLandscapeText"
                android:layout_margin="10dp"
                android:src="@drawable/img_landscape_active"
                android:padding="20dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="조경시설"
                android:textSize="15dp"
                android:id="@+id/ClaimMainCategoryLandscapeText"
                app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryLandscape"
                android:fontFamily="@font/notosanscjkkr_medium"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/ClaimMainCategorySecurityLayout"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintEnd_toEndOf="@id/ClaimMainCategorySecurityLayout"
        app:layout_constraintTop_toBottomOf="@id/ClaimMainCategorySecurityLayout"
        android:layout_marginTop="15dp"
        android:background="@drawable/concierge_main_selector_view"
        android:id="@+id/ClaimMainCategoryFacilityLayout">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/ClaimMainCategoryFacility"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintDimensionRatio="H,1:1"
                android:src="@drawable/img_facility_active"
                android:padding="20dp"
                app:layout_constraintBottom_toTopOf="@id/ClaimMainCategoryFacilityText"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:id="@+id/ClaimMainCategoryFacilityText"
                app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryFacility"
                android:text="공동시설"
                android:textSize="15dp"
                android:fontFamily="@font/notosanscjkkr_medium"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintStart_toStartOf="@id/ClaimMainCategoryLandscapeLayout"
        app:layout_constraintEnd_toEndOf="@id/ClaimMainCategoryLandscapeLayout"
        android:background="@drawable/concierge_main_selector_view"
        app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryLandscapeLayout"
        android:id="@+id/ClaimMainCategoryEntranceLayout"
        android:layout_marginTop="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_margin="3dp"
            android:background="@drawable/shape_grey_radius_10">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/img_entrance_active"
                android:padding="20dp"
                app:layout_constraintDimensionRatio="H ,1:1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintBottom_toTopOf="@id/ClaimMainCategoryEntranceText"
                android:id="@+id/ClaimMainCategoryEntrance" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="공동현관/복도"
                android:textSize="15dp"
                android:id="@+id/ClaimMainCategoryEntranceText"
                app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryEntrance"
                android:fontFamily="@font/notosanscjkkr_medium"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintStart_toStartOf="@id/ClaimMainCategoryFacilityLayout"
        app:layout_constraintEnd_toEndOf="@id/ClaimMainCategoryFacilityLayout"
        android:background="@drawable/concierge_main_selector_view"
        app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryFacilityLayout"
        android:id="@+id/ClaimMainCategoryFlawLayout"
        android:layout_marginTop="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_margin="3dp"
            android:background="@drawable/shape_grey_radius_10">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="H,1:1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:src="@drawable/img_flaw_active"
                android:padding="20dp"
                android:id="@+id/ClaimMainCategoryFlaw"
                app:layout_constraintBottom_toTopOf="@id/ClaimMainCategoryFlawText"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="세대하자/민원"
                android:textSize="15dp"
                android:id="@+id/ClaimMainCategoryFlawText"
                app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryFlaw"
                android:fontFamily="@font/notosanscjkkr_medium"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintStart_toStartOf="@id/ClaimMainCategoryEntranceLayout"
        app:layout_constraintEnd_toEndOf="@id/ClaimMainCategoryEntranceLayout"
        android:background="@drawable/concierge_main_selector_view"
        app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryEntranceLayout"
        android:id="@+id/ClaimMainCategoryEtcLayout"
        android:layout_marginTop="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_margin="3dp"
            android:background="@drawable/shape_grey_radius_10">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/ic_category_etc"
                app:layout_constraintDimensionRatio="H ,1:1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintBottom_toTopOf="@id/ClaimMainCategoryEtcText"
                android:id="@+id/ClaimMainCategoryEtc" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="기타"
                android:textSize="15dp"
                android:id="@+id/ClaimMainCategoryEtcText"
                app:layout_constraintTop_toBottomOf="@id/ClaimMainCategoryEtc"
                android:fontFamily="@font/notosanscjkkr_medium"
                android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
</layout>