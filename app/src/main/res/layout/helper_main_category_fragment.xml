<?xml version="1.0" encoding="utf-8"?>
<layout>

<data>
    <variable
        name="model"
        type="com.moa.moakotlin.ui.concierge.category.HelperMainCategoryViewModel" />
</data>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.concierge.category.HelperMainCategoryFragment">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toStartOf="@id/HelperMainCategoryEduLayout"
    android:background="@drawable/shape_grey_radius_10"
    app:layout_constraintDimensionRatio="H,1:1"
    android:layout_margin="15dp"
    android:id="@+id/HelperMainCategoryKidLayout"
    app:layout_constraintTop_toTopOf="parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="5dp"
        android:background="@drawable/shape_grey_radius_10"
        app:layout_constraintStart_toStartOf="@id/HelperMainCategoryKidLayout"
        app:layout_constraintEnd_toEndOf="@id/HelperMainCategoryKidLayout"
        app:layout_constraintTop_toTopOf="@id/HelperMainCategoryKidLayout"
        app:layout_constraintBottom_toBottomOf="@id/HelperMainCategoryKidLayout"
        app:layout_constraintDimensionRatio="H,1:1">
        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="15dp"
            android:src="@drawable/ic_category_kid"
            android:id="@+id/HelperMainCategoryKid"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="육아"
            android:fontFamily="@font/notosanscjkkr_medium"
            app:layout_constraintStart_toStartOf="@id/HelperMainCategoryKid"
            app:layout_constraintEnd_toEndOf="@id/HelperMainCategoryKid"
            app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="#000000"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:id="@+id/HelperMainCategoryEduLayout"
    app:layout_constraintDimensionRatio="H,1:1"
    app:layout_constraintStart_toEndOf="@id/HelperMainCategoryKidLayout"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_margin="15dp"
    android:background="@drawable/shape_grey_radius_10">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/shape_grey_radius_10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="H,1:1"
        android:layout_margin="3dp">
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintDimensionRatio="H,1:1"
        android:layout_margin="15dp"
        android:src="@drawable/ic_category_edu"
        android:id="@+id/HelperMainCategoryEdu"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="교육"
        android:fontFamily="@font/notosanscjkkr_medium"
        app:layout_constraintStart_toStartOf="@id/HelperMainCategoryEdu"
        app:layout_constraintEnd_toEndOf="@id/HelperMainCategoryEdu"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="#000000"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintStart_toStartOf="@id/HelperMainCategoryKidLayout"
    app:layout_constraintEnd_toEndOf="@id/HelperMainCategoryKidLayout"
    app:layout_constraintTop_toBottomOf="@id/HelperMainCategoryKidLayout"
    android:id="@+id/HelperMainCategoryPetLayout"
    app:layout_constraintDimensionRatio="H,1:1"
    android:background="@drawable/shape_grey_radius_10"
    android:layout_marginTop="15dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/shape_grey_radius_10"
        app:layout_constraintDimensionRatio="H,1:1"
        android:layout_margin="3dp">
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintDimensionRatio="H,1:1"
        android:layout_margin="15dp"
        android:src="@drawable/ic_category_pet"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="반려동물케어"
        android:fontFamily="@font/notosanscjkkr_medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/moaBlack"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/HelperMainCategoryEduLayout"
        app:layout_constraintEnd_toEndOf="@id/HelperMainCategoryEduLayout"
        app:layout_constraintTop_toBottomOf="@id/HelperMainCategoryEduLayout"
        android:layout_marginTop="15dp"
        app:layout_constraintDimensionRatio="H,1:1"
        android:background="@drawable/shape_grey_radius_10"
        android:id="@+id/HelperMainCategoryInteriorLayout">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/shape_grey_radius_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="H,1:1"
            android:layout_margin="3dp">
<ImageView
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_margin="15dp"
    app:layout_constraintDimensionRatio="H,1:1"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:src="@drawable/ic_category_kid"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="인테리어"
            android:fontFamily="@font/notosanscjkkr_medium"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@color/moaBlack"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintDimensionRatio="H,1:1"
    app:layout_constraintStart_toStartOf="@id/HelperMainCategoryPetLayout"
    app:layout_constraintEnd_toEndOf="@id/HelperMainCategoryPetLayout"
    app:layout_constraintTop_toBottomOf="@id/HelperMainCategoryPetLayout"
    android:layout_marginTop="15dp"
    android:background="@drawable/shape_grey_radius_10"
    android:id="@+id/HelperMainCategoryEtcLayout">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/shape_grey_radius_10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="H,1:1"
        android:layout_margin="3dp">
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/ic_category_etc"
        android:layout_margin="15dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="기타"
        android:fontFamily="@font/notosanscjkkr_medium"
        android:textColor="@color/moaBlack"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
</layout>